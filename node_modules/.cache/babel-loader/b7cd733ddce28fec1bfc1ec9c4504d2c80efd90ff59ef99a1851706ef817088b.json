lideIndex = ref(0); // Track the current slide index\n\n    // Fetch products from API\n    const fetchProducts = async () => {\n      try {\n        const response = await axios.get('http://127.0.0.1:5000/api/products'); // Replace 'your-api-endpoint' with the actual API endpoint to fetch products\n        products.value = response.data; // Assuming the API response is an array of product objects\n      } catch (error) {\n        console.error('Failed to fetch products:', error);\n      }\n    };\n\n    // Function to go to the next slide\n    const goToNextSlide = () => {\n      currentSlideIndex.value = (currentSlideIndex.value + 1) % products.value.length;\n    };\n    const openProduct = product => {\n      // Navigate to the product view page for the clicked product\n      // and pass the product ID as a route parameter\n      console.log(product.id);\n      router.push({\n        name: 'ProductView',\n        params: {\n          id: product.id\n        }\n      });\n    };\n    onMounted(() => {\n      setInterval(goToNextSlide, 5000);\n    });\n    return {\n      openProduct,\n      products,\n      goToNextSlide\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","ref","onMounted","Carousel","Slide","axios","useRouter","name","components","setup","products","router","currentSlideIndex","fetchProducts","response","get","value","data","error","console","goToNextSlide","length","openProduct","product","log","id","push","params","setInterval"],"sources":["/home/sezfabian/Devs/The_Tv_Guys/web_app/src/components/ProductsCarousel.vue"],"sourcesContent":["<template>\n  <Carousel :itemsToShow=\"3\" :wrapAround=\"true\" :transition=\"500\" class=\"carousel\">\n    <button @click=\"goToNextSlide\">Next</button>\n    <Slide v-for=\"product in products\" :key=\"product.id\">\n      <div @click=\"openProduct(product)\">\n        <img :src=\"product.Image\" class=\"slide-img-top\" alt=\"Product Image\" width=\"500\">\n            \n        <div class=\"card